<h1><%= t('core.listing')%></h1>

<table>
  <tr>
    <th><%= t('core.city')%></th>
    <th><%= t('core.community')%></th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @cores.each do |core| %>
  <tr>
    <td><%= core.city%></td>
    <td><%= core.community%></td>
    <td><%= link_to t('show'), core %></td>
    <td><%= link_to t('edit'), edit_core_path(core) %></td>
    <td><%= link_to t('delete'), core, :confirm => t('are_you_sure'), :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

