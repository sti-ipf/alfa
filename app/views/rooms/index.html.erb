<h1><%= t('room.listing')%></h1>

<table>
  <tr>
    <th><%= t('room.name')%></th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @rooms.each do |room| %>
  <tr>
    <td><%= room.name %></td>
    <td><%= link_to t('show'), room %></td>
    <td><%= link_to t('edit'), edit_room_path(room) %></td>
    <td><%= link_to t('delete'), room, :confirm => t('are_you_sure'), :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

